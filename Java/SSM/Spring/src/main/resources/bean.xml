    <!--    id 唯一标识-->
    <!--    class 权限名-->
    <bean id="accountService" class="U3_KonwSpring.impl.AccountServiceImpl"></bean>

    <bean id="accountDao" class="U3_KonwSpring.impl.AccountDaoImpl"></bean>
</beans>